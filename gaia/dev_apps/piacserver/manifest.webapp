{
	"name": "PortIAC Test Server",
	"version": "1.3",
	"type": "certified",
	"description": "Test PortIAC",
	"developer": {
        "name": "yxu",
        "url": "http://yxu"
    },
	"launch_path": "/index.html",
	"icons": {
        "16": "/img/icons/icon-16.png",
		"32": "/img/icons/icon-32.png",
        "48": "/img/icons/icon-48.png",
		"60": "/img/icons/icon-60.png",
		"90": "/img/icons/icon-90.png",
		"120": "/img/icons/icon-120.png",
        "128": "/img/icons/icon-128.png",
		"256": "/img/icons/icon-256.png"
    },
	"default_locale": "en",
	"orientation": ["portrait"],
	"permissions": {
		"device-storage:pictures": {
			"access": "createonly",
			"description" : "To save image"
		}
    },

  "connections": {
    "app-piacserver.gaiamobile.org/TestTimeService": {
      "handler_path": "/index.html",
      "description": "benchmark piac",
      "rules": {}
    },
    "app-piacserver.gaiamobile.org/TestInterAppService": {
      "handler_path": "/index.html",
      "description": "test piac",
      "rules": {}
    }
  }
}
